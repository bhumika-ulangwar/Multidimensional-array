#include <iostream>
using namespace std;

int main() {
    int matrix[3][3];

    cout << "Enter matrix :";
    for (int i = 0; i < 3; i++)
        for (int j = 0; j < 3; j++)
            cin >> matrix[i][j];

    cout << "Matrix is:\n";
    for (int i = 0; i < 3; i++) {
        for (int j = 0; j < 3; j++)
            cout << matrix[i][j] << " ";
        cout << endl;
    }

    return 0;
}

//OUTPUT
Enter matrix :2 3 4
5 6 8
5 9 7
Matrix is:
2 3 4 
5 6 8 
5 9 7 
